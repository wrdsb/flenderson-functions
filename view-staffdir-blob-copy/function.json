{
    "bindings": [
        {
            "name": "req",
            "type": "httpTrigger",
            "authLevel": "function",
            "direction": "in",
            "methods": [
                "post"
            ]
        },
        {
            "name": "inBlob",
            "type": "blob",
            "path": "ipps-view-staffdir/now.json",
            "connection": "wrdsbpanama_STORAGE",
            "direction": "in"
        },
        {
            "name": "outBlob",
            "type": "blob",
            "path": "ipps/view-staffdir-raw.json",
            "connection": "wrdsbflenderson_STORAGE",
            "direction": "out"
        },
        {
            "name": "flynnGrid",
            "type": "eventHub",
            "eventHubName": "flynn-events",
            "connection": "wrdsb-flynn_EVENTHUB",
            "direction": "out"
        },
        {
            "name": "res",
            "type": "http",
            "direction": "out"
        }
    ],
    "disabled": false
}
